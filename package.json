    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ci": "npm run clean && npm run test && npm run build:prod",
    "build:doc": "./node_modules/.bin/compodoc -p tsconfig.json --theme material -d dist/apps/documentation/compodoc ",
